{
    "type": "widget",
    "class": "qx.ui.container.Composite",
    "layout": "qx.ui.layout.VBox",
    "addOptions": {
        "flex": 1
    },
    "layoutConfig": {
        "spacing": 5
    },
    "extensions": {
        "resources": {
            "favicon": "@Ligature/server"
        },
        "tabConfig": {
            "title": "tr('Samba domain')",
            "icon": "favicon"
        }
    },
    "children": [{
        "class": "gosa.ui.widgets.GroupBox",
        "layout": "qx.ui.layout.Flow",
        "properties": {
            "legend": "tr('TODO')"
        },
        "children": [{
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaDomainName",
            "properties": {
                "text": "tr('Windows NT domain')"
            }
        }, {
            "class": "gosa.ui.widgets.QLineEditWidget",
            "modelPath": "sambaDomainName"
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaSID",
            "properties": {
                "text": "tr('Security ID')"
            }
        }, {
            "class": "gosa.ui.widgets.QLineEditWidget",
            "modelPath": "sambaSID"
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaAlgorithmicRidBase",
            "properties": {
                "text": "tr('Base at which the samba RID generation algorithm should operate')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaAlgorithmicRidBase",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaNextUserRid",
            "properties": {
                "text": "tr('Next NT rid to give our for users')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaNextUserRid",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaMinPwdLength",
            "properties": {
                "text": "tr('Minimal password length (default: 5)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaMinPwdLength",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaPwdHistoryLength",
            "properties": {
                "text": "tr('Length of Password History Entries (default: 0 => off)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaPwdHistoryLength",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaLogonToChgPwd",
            "properties": {
                "text": "tr('Force Users to logon for password change (default: 0 => off, 2 => on)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaLogonToChgPwd",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaMaxPwdAge",
            "properties": {
                "text": "tr('Maximum password age, in seconds (default: -1 => never expire passwords)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaMaxPwdAge",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaMinPwdAge",
            "properties": {
                "text": "tr('Minimum password age, in seconds (default: 0 => allow immediate password change)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaMinPwdAge",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaLockoutDuration",
            "properties": {
                "text": "tr('Lockout duration in minutes (default: 30, -1 => forever)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaLockoutDuration",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaLockoutObservationWindow",
            "properties": {
                "text": "tr('Reset time after lockout in minutes (default: 30)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaLockoutObservationWindow",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaLockoutThreshold",
            "properties": {
                "text": "tr('Lockout users after bad logon attempts (default: 0 => off)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaLockoutThreshold",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaForceLogoff",
            "properties": {
                "text": "tr('Disconnect Users outside logon hours (default: -1 => off, 0 => on)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaForceLogoff",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaRefuseMachinePwdChange",
            "properties": {
                "text": "tr('Allow Machine Password changes (default: 0 => off)')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaRefuseMachinePwdChange",
            "addOptions": {
                "lineBreak": true
            }
        }, {
            "class": "gosa.ui.widgets.QLabelWidget",
            "buddyModelPath": "sambaNextRid",
            "properties": {
                "text": "tr('Next NT rid to give out for anything')"
            }
        }, {
            "class": "gosa.ui.widgets.QSpinBoxWidget",
            "modelPath": "sambaNextRid"
        }]
    }]
}
