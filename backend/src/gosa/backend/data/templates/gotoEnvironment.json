{
  "type": "widget",
  "layout": "qx.ui.layout.Grow",
  "extensions": {
    "tabConfig": {
      "title": "tr('Schreibtisch')"
    }
  },
  "class": "qx.ui.container.Composite",
  "children": [
    {
      "class": "qx.ui.container.Composite",
      "layout": "qx.ui.layout.Grid",
      "layoutConfig": {
        "spacingX": "CONST_SPACING_X",
        "spacingY": "CONST_SPACING_Y"
      },
      "extensions": {
        "layoutOptions": {
          "columnFlex": {
            "column": 1,
            "flex": 1
          }
        }
      },
      "children": [
        {
          "addOptions": {
            "row": 0,
            "column": 0
          },
          "buddyModelPath": "gotoXResolution",
          "properties": {
            "text": "tr('Resolution')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoXResolution",
          "addOptions": {
            "row": 0,
            "column": 1
          },
          "properties": {
            "tabIndex": 1
          },
          "class": "gosa.ui.widgets.QComboBoxWidget"
        },
        {
          "addOptions": {
            "row": 1,
            "column": 0
          },
          "buddyModelPath": "gotoDefaultLanguage",
          "properties": {
            "text": "tr('Preferred language')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoDefaultLanguage",
          "addOptions": {
            "row": 1,
            "column": 1
          },
          "properties": {
            "tabIndex": 2
          },
          "class": "gosa.ui.widgets.QComboBoxWidget"
        },
        {
          "addOptions": {
            "row": 2,
            "column": 0
          },
          "buddyModelPath": "gotoLogonScript",
          "properties": {
            "text": "tr('Logon script')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoLogonScript",
          "addOptions": {
            "row": 2,
            "column": 1
          },
          "properties": {
            "tabIndex": 3,
            "height": 300
          },
          "class": "gosa.ui.widgets.QEditorWidget",
          "extensions": {
            "guiProperties": {
              "minimap": {
                "enabled": false
              }
            }
          }
        },
        {
          "addOptions": {
            "row": 3,
            "column": 0
          },
          "buddyModelPath": "gotoDefaultPrinter",
          "properties": {
            "text": "tr('Default printer')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoDefaultPrinter",
          "addOptions": {
            "row": 3,
            "column": 1
          },
          "properties": {
            "tabIndex": 4,
            "multivalue": false
          },
          "class": "gosa.ui.widgets.QTableWidgetWidget"
        }
      ]
    }
  ]
}